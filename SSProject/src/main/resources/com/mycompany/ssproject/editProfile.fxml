<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.geometry.Insets?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mycompany.ssproject.EditprofileController">
    <children>
        <Label text="Edit Profile" />
      <Label layoutX="30.0" layoutY="30.0" text="Username:" />
      <TextField fx:id="usernameUpdateForm" promptText="Enter a Username" />
      <Label text="Password:" />
      <PasswordField fx:id="updatePasswordForm" promptText="Enter a Password" />
      <Button fx:id="updateButton" layoutX="91.0" layoutY="225.0" onAction="#updateForm" text="Update" />
      <Button fx:id="backtomainButton" onAction="#switchToMain" text="Back" />
    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
</VBox>

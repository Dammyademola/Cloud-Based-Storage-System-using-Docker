<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.geometry.Insets?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mycompany.ssproject.LoginController">
   <children>
      <Label text="Login" />
      <Label text="Username:">
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </Label>
      <TextField fx:id="usernamelogin" />
      <Label layoutX="38.0" layoutY="65.0" text="Password:">
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </Label>
      <PasswordField fx:id="passwordlogin" />
      <Button fx:id="loginButton" mnemonicParsing="false" onAction="#ProcessLogin" text="Login" textAlignment="CENTER" />
      <Button fx:id="registerpageButton" onAction="#switchToRegister" text="Sign up now" />
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
